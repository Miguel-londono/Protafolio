---
const project = Astro.props;
---

<div
  class="sm:flex gap-8 p-4 border-2 rounded-lg hover:opacity-70 ease-in-out transition hover:scale-[101%] cursor-pointer duration-500"
>
  <img src={project.image.url} alt="img-profile" class="rounded-lg h-32 w-46" />
  <div class=" md:flex gap-3 aline">
      <h1 class="font-bold text-2xl"> {project.title}</h1> 
      <p>{project.date}</p>
  </div>
  <ul>
    <li></li>
  </ul>
</div>
